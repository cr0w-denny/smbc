    "clean": "rm -rf tsp-output",
    "test": "echo 'No tests for API schemas'"
  },
  "dependencies": {
    "@typespec/compiler": "^0.55.0",
    "@typespec/http": "^0.55.0",
    "@typespec/rest": "^0.55.0",
    "@typespec/openapi3": "^0.55.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "files": [
    "tsp-output/@typespec/openapi3/openapi.json"
  ]
}
