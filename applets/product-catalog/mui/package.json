{
  "name": "@smbc/product-catalog-mui",
  "version": "0.0.1",
  "description": "Material-UI React components for product catalog",
  "main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "smbc-applet",
    "smbc-mui-applet"
  ],
