{
  "name": "@smbc/filter-demo-mui",
  "version": "0.0.1",
  "type": "module",
  "keywords": [
    "smbc-applet",
    "smbc-mui-applet"
  ],
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
