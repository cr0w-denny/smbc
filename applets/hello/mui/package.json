{
  "name": "@smbc/hello-mui",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "smbc-applet",
    "smbc-mui-applet"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@smbc/applet-core": "*",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  },
  "publishConfig": {
    "registry": "http://localhost:4873",
    "access": "public"
  },
  "peerDependencies": {
    "@mui/material": "^7.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "smbc": {
    "applet": {
      "id": "hello",
      "name": "Hello World",
      "description": "Simple hello world applet for testing and demos",
