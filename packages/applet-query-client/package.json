{
  "name": "@smbc/applet-query-client",
  "version": "0.0.0",
  "description": "TanStack Query client provider and OpenAPI client utilities for applet packages",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
