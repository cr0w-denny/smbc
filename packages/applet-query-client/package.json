    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.81.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.81.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "typescript": "^5.8.3"
  }
