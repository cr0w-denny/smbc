{
  "name": "@smbc/mui-applet-devtools",
  "version": "0.0.0",
  "description": "MUI development components and tools for SMBC applets",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "setup-mocks": "./bin/setup-mocks.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "scripts",
    "bin"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "lint": "echo 'Lint: mui-applet-devtools'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@smbc/applet-core": "*",
    "@smbc/applet-devtools": "*",
    "@smbc/mui-components": "*",
    "@smbc/shared-deps": "*",
    "swagger-ui-react": "^5.25.2",
    "github-markdown-css": "^5.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}