{
  "name": "@smbc/mui-applet-core",
  "version": "0.0.1",
  "description": "MUI-specific stateful applet components for SMBC applications",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "echo 'Lint: mui-applet-core'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@smbc/mui-components": "*"
  },
  "devDependencies": {
    "@smbc/ui-core": "*",
    "@smbc/applet-core": "*",
    "@smbc/applet-host": "*",
    "@smbc/dataview": "*",
    "@smbc/dataview-mui": "*",
    "@types/node": "^20.11.17",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@smbc/ui-core": "*",
    "@smbc/applet-core": "*",
    "@smbc/applet-host": "*",
    "@smbc/mui-components": "*",
    "@smbc/dataview": "*",
    "@smbc/dataview-mui": "*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "publishConfig": {
    "registry": "http://localhost:4873",
    "access": "public"
  }
}
