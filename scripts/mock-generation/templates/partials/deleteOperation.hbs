    const entityId = params.{{pathParam}} as string;
    
    // Check if item exists
    if (!{{lowerEntityName}}DataStore.has(entityId)) {
      return HttpResponse.json(
        { error: 'Not found', message: '{{entityName}} not found' },
        { status: 404 }
      );
    }
    
    // Remove from store
    {{lowerEntityName}}DataStore.delete(entityId);
    
    return HttpResponse.json({ 
      message: `{{entityName}} ${entityId} deleted successfully` 
    });